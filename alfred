#!/bin/sh
shuf -n 1 quotes.csv
if [ $1 = "dev" ] || [ -z $1 ]; then
if [-n]; then shift; fi
echo "Alfred! running dev $1"
docker-compose -f docker-compose.yml -f dev.yml up $1

elif [ $1 = "prod" -o $1 = "production" ]; then
shift
echo "Alfred! running production $1"
docker-compose -f docker-compose.yml -f production.yml up $1

fi
